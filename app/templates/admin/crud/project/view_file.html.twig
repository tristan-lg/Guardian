{# @var ea \EasyCorp\Bundle\EasyAdminBundle\Context\AdminContext #}
{% extends '@!EasyAdmin/page/content.html.twig' %}

{% block content_title %}{{ project.name }} > {{ fileKey }}{% endblock %}

{% block page_actions %}
    <a href="{{ ea_url()
        .setController('App\\Controller\\Admin\\Crud\\ProjectCrudController')
        .setAction('detail')
        .setEntityId(ea.entity.instance.id)
    }}" class="btn btn-secondary">Retour au projet</a>
{% endblock %}

{% block page_content %}
    <div style="border: 2px solid black">
        <pre>{{ fileContent|json_encode(constant('JSON_PRETTY_PRINT')) }}</pre>
    </div>
{% endblock %}

